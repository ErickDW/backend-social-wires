{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"redirects": [
		{
			"source": "/(.*)",
			"has": [
				{
					"type": "header",
					"key": "Access-Control-Allow-Credentials",
					"value": "true"
				},
				{
					"type": "header",
					"key": "Access-Control-Allow-Origin",
					"value": "https://social-wires.web.app"
				},
				{
					"type": "header",
					"key": "Access-Control-Allow-Methods",
					"value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
				}
			],
			"destination": "src/main.ts"
		}
	]
}
