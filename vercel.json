{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"headers": {
				"Access-Control-Allow-Origin": [
					"https://social-wires.web.app",
					"http://localhost:4200"
				],
				"Access-Control-Allow-Credentials": true
			},
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE",
				"OPTIONS",
				"HEAD",
				"PATCH"
			],
			"credentials": true
		}
	]
}
