{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"headers": {
				"Access-Control-Allow-Origin": "https://social-wires.web.app"
			},
			"methods": [
				"GET",
				"POST",
				"PUT",
				"DELETE",
				"OPTIONS",
				"HEAD",
				"PATCH"
			],
			"continue": true
		},
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"headers": { "Access-Control-Allow-Credentials": "true" },
			"continue": true
		},
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"headers": { "Access-Control-Allow-Credentials": "true" },
			"continue": true
		},
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"headers": { "Cookie": "" },
			"continue": true
		}
	]
}
