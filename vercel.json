{
	"version": 2,
	"builds": [
		{
			"src": "src/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/main.ts",
			"headers": {
				"Access-Control-Allow-Origin": "*"
			},
			"methods": ["GET", "POST", "PUT", "DELETE"],
			"origin": [
				"*",
				"https://www.google.com",
				"http://localhost:4200",
				"http://localhost:4200/signin"
			]
		}
	]
}
